{
    "name": "book_app",
    "version": "1.0.0",
    "description": "Simple Book app CRUD (Express + MySQL)",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "db:status": "liquibase --defaultsFile=liquibase/liquibase.properties status",
        "db:update": "liquibase --defaultsFile=liquibase/liquibase.properties update",
        "db:rollback": "liquibase --defaultsFile=liquibase/liquibase.properties rollbackCount 1"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "mysql2": "3.15.3"
    },
    "devDependencies": {
        "liquibase": "^4.28.1",
        "nodemon": "^2.0.22"
    }
}
